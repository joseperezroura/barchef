<div class="container">
	<div class="row row-profile-main">
		<div class="col-sm-3">
			<div class="well well-main-profile">
				<div class="well well-inside">
					<ul>
						<li><img src="../assets/john.png" class="img-thumbnail img-user-thumbnail" alt="Image"></li>
							<li><h3><%= @user.first_name %> <%= @user.last_name %></h3></li>
					</ul>
				</div>
				<ul>
					<li><%= @user.location %></li>
					<li><%= @user.bio %></li>
					<li><%= @user.industry %></li>
					<li><%= @user.company %></li>
					<li>Recipes</li>
				</ul>
			</div>
		</div>
		<div class="col-sm-9 col-profile-main">
			<div class="well well-main-profile">
				<div class="well well-inside">
				</div>
			</div>
		</div>
	</div>
</div>